. ****************************************************************************
. * Summary Table - Renters 
. ****************************************************************************
. 
. egen med_rent_1_exp = median(rent_1_exp), by(survey)

. label var med_rent_1_exp "Median Rent to Exp - de jure (%)"

. egen med_rent_2_exp = median(rent_2_exp), by(survey)

. label var med_rent_2_exp "Median Rent to Exp - de facto (%)"

. 
. label var rent_1 "Mean Rent - de jure (Rs.)"

. label var rent_2 "Mean Rent - de facto (Rs.)"

. 
. egen med_rent_1 = median(rent_1), by(survey)

. label var med_rent_1 "Median Rent - de jure (Rs.)"

. egen med_rent_2 = median(rent_2), by(survey)

. label var med_rent_2 "Median Rent - de facto (Rs.)"

. 
. label var rent_1_exp "Mean Rent to Exp - de jure (%)"

. label var rent_2_exp "Mean Rent to Exp - de facto (%)"

. 
. 
. local var_summary renter_1 rent_1 med_rent_1  rent_1_exp  med_rent_1_exp  renter_2  rent_2 med_rent_2 rent_2_exp med_rent_2_exp

. 
. local nss_round "NSS50 NSS61 NSS68"

. 
. foreach survey in `nss_round'{
  2. qui eststo `survey': quietly estpost summarize `var_summary' [aw=hhwt] if survey == "`survey'", de
  3. qui eststo `survey'_u: quietly estpost summarize `var_summary' [aw=hhwt] if survey == "`survey'" & sector ==2, de
  4. }

