. ****************************************************************************
. * Generate the time series table of housing conditions
. ****************************************************************************
. use "${r_output}\NSS_housing_condition.dta",clear

. 
. ********************
. *prepare the data***
. ********************
. 
. *By survey generate consumption quintile for All India households****
. xtile qt = hh_umce [aw = hh_weight] if survey == "NSS49", nq(5) 

. 
. foreach survey in NSS58 NSS65 NSS69 NSS76 {
  2.   xtile temp_qt_`survey' = hh_umce [aw = hh_weight] if survey == "`survey'", nq(5) 
  3.   qui replace qt = temp_qt_`survey' if survey == "`survey'" 
  4. }

. drop temp*

. 
. *produce delta between q1 and q5 for each variable****
. //ssc inst asgen  //the package for weighted mean 
. 
. local var_list hh_size in_room in_wall_permanent in_roof_permanent in_floor_permanent in_all_permanent in_sep_kitch in_flat in_size in_ppl_room in_ppl_area h20_improved san_improved san_flush_private

. local nss_round "NSS49 NSS58 NSS65 NSS69 NSS76" 

. 
. foreach var in `var_list' {
  2. //sort out the first and fifth quintile
.  forvalues i = 1(4)5 {
  3.  qui gen `var'_`i' = `var' *(qt == `i')
  4.  qui replace `var'_`i'  = . if (qt != `i')
  5.  }
  6. }

. 
. foreach var in `var_list' {
  2. qui gen `var'_dt = .
  3.  foreach survey in `nss_round' {
  4.   forvalues i = 1(4)5 {
  5.    qui asgen `var'_`i'_`survey'_mean = `var'_`i' if survey == "`survey'",w(hh_weight)     
  6.   }
  7.   qui gen temp_`var'_`survey'_dt = (`var'_5_`survey'_mean  - `var'_1_`survey'_mean)/`var'_1_`survey'_mean 
  8.   qui replace `var'_dt = temp_`var'_`survey'_dt if survey == "`survey'"
  9.  }
 10. }

. 
. foreach var in `var_list' {
  2.  local label_old: var label `var'
  3.  local label = " Delta  Q5 & Q1" + ": `label_old'" 
  4.  label var `var'_dt "`label'"
  5. }

. 
